<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label>Desde aquí puede registrarse en AdMe!</g:Label>
		<g:TextBox ui:field="correo">Correo</g:TextBox>
		<g:PasswordTextBox ui:field="contrasena">Contraseña</g:PasswordTextBox>
		<g:TextBox ui:field="nombre">Nombre</g:TextBox>
		<g:TextBox ui:field="apellido1">Primer Apellido</g:TextBox>
		<g:TextBox ui:field="apellido2">Segundo Apellido</g:TextBox>
		<g:Button styleName="{style.important}" ui:field="registerButton">RegistraMe!</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 